{
  "name": "rpa-web-ui",
  "version": "1.0.0",
  "description": "Vue 3 TypeScript component library for advanced data grids and listboxes",
  "type": "module",
  "main": "./dist/rpa-web-ui.umd.js",
  "module": "./dist/rpa-web-ui.js",
  "types": "./dist/rpa-web-ui.d.ts",
  "exports": {
    ".": {
      "types": "./dist/rpa-web-ui.d.ts",
      "import": "./dist/rpa-web-ui.js",
      "require": "./dist/rpa-web-ui.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "typecheck": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "^3.4.0",
    "pinia": "^2.1.7"
  },
  "dependencies": {
    "@imengyu/vue3-context-menu": "^1.5.2",
    "@microsoft/signalr": "^8.0.0",
    "@vueuse/core": "^10.7.0",
    "axios": "^1.6.2",
    "mitt": "^3.0.1",
    "vue-virtual-scroller": "^2.0.0-beta.8"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vue-tsc": "^3.1.5"
  },
  "keywords": [
    "vue",
    "vue3",
    "typescript",
    "datagrid",
    "table",
    "listbox",
    "rpa",
    "component-library"
  ],
  "author": "",
  "license": "MIT"
}
